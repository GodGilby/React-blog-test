{
    "compilerOptions": {
      "jsx": "react",
      "target": "es6",
      "module": "esnext",
      "moduleResolution": "node",
      "isolatedModules": false,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "strict": true,
      "sourceMap": true,
      "noEmit": true
    },
    "include": ["src/**/*"]
  }